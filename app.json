{
  "expo": {
    "name": "CBH Online",
    "slug": "cbh-youth-online-mobile",
    "version": "1.5.3",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "com.fatties.youth",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./app/assets/2026-ios.png",
        "light": "./app/assets/2026-ios.png",
        "tinted": "./app/assets/2026-ios.png"
      },
      "bundleIdentifier": "com.fatties.youth",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "com.fatties.youth",
              "host": "oauth"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/2026-android.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fatties.youth"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "172eec8c-8c58-453c-a375-258412496e09"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/172eec8c-8c58-453c-a375-258412496e09"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Cho phép CBH Youth Online truy cập camera của bạn",
          "microphonePermission": "Cho phép CBH Youth Online truy cập microphone của bạn",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./app/assets/notification.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      "expo-web-browser",
      "expo-apple-authentication"
    ],
    "notification": {
      "icon": "./app/assets/notification.png",
      "color": "#ffffff"
    }
  }
}
